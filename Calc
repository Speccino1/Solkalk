                            <script type="text/javascript">
                                require(['jquery'], function($) {
                                    $(document).ready(function() {
                                        $('.mainTab-panel ul.category-brands-list').hide();
                                        $('.mainTab-panel ul.category-brands-list:first').show();
                                        var datavalue = $('.mainTab-panel ul.category-brands-list:first').data('value');
                                        $('.mainTab-panel ul.menu-item-ul li').removeClass('active');
                                        $('.mainTab-panel ul.menu-item-ul li[data-id="' + datavalue + '"]').addClass('active');
                                        $('.mainTab-panel .menu-item-ul li').click(function() {
                                            var dataId = $(this).data('id');
                                            $('.mainTab-panel ul.menu-item-ul li').removeClass('active');
                                            $(this).addClass('active');
                                            $('.mainTab-panel ul.category-brands-list').hide();
                                            $('.mainTab-panel ul.category-brands-list[data-value="' + dataId + '"]').show();
                                        });
                                    });
                                });
                            </script>
