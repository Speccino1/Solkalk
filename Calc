        <script type="text/javascript">
            require(['jquery', 'mage/cookies', 'fancybox/js/jquery.fancybox'], function($) {
                $(document).ready(function() {
                    if ($("body").hasClass("cms-index-index")) {
                        var check_cookie = $.cookie('product_promotion');
                        if (window.location != window.parent.location) {
                            $('#product_promotion').remove();
                        } else {
                            if (check_cookie == null || check_cookie == 'shown') {
                                setTimeout(function() {
                                    beginPromotionForm();
                                }, 1000);
                            }
                            $('#product_promotion_dont_show_again').on('change', function() {
                                if ($(this).length) {
                                    var check_cookie = $.cookie('product_promotion');
                                    if (check_cookie == null || check_cookie == 'shown') {
                                        $.cookie('product_promotion', 'dontshowitagain');
                                    } else {
                                        $.cookie('product_promotion', 'shown');
                                        beginPromotionForm();
                                    }
                                } else {
                                    $.cookie('product_promotion', 'shown');
                                }
                            });
                        }
                    }
                });

                function beginPromotionForm() {
                    $.fancybox({
                        'padding': '0px',
                        'autoScale': true,
                        'transitionIn': 'fade',
                        'transitionOut': 'fade',
                        'type': 'inline',
                        'href': '#product_promotion',
                        'onComplete': function() {
                            $.cookie('product_promotion', 'shown');
                        },
                        'tpl': {
                            closeBtn: '<a title="Close" class="fancybox-item fancybox-close fancybox-promotion-close" href="javascript:;"></a>'
                        },
                        'helpers': {
                            overlay: {
                                locked: false
                            }
                        }
                    });
                    $('#product_promotion').trigger('click');
                }
            });
        </script>
